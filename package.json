{
	"name": "contesthelper",
	"displayName": "ContestHelper",
	"description": "VS code plugin for Competitive programming",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.startServer",
				"title": "ContestHelper: Start Server"
			},
			{
				"command": "extension.editTestFile",
				"title": "ContestHelper: Edit Test File"
			}
		],
		"configuration": {
			"title": "Contest Helper Configuration",
			"properties": {
				"contesthelper.directory-path": {
					"type": "string",
					"default": "D:\\",
					"description": "Set the path of the directory in which you want the extension to work in",
					"scope": "resource"
				}
			}
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0",
		"body-parser": "^1.18.2",
		"express": "^4.16.3"
	}
}
